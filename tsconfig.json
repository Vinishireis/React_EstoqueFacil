{
  "compilerOptions": {
    "jsx": "react",
    "allowJs": true,
    "target": "es5",
    "lib": ["dom", "es2015"],
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true
  },
  "include": ["./src/**/*", "./App.tsx"],
  "exclude": ["node_modules"],
  "extends": "expo/tsconfig.base"
}
